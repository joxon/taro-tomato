{
  "extends": ["taro", "standard"],
  "rules": {
    "no-unused-vars": ["off", { "varsIgnorePattern": "Taro" }],
    "react/jsx-filename-extension": [
      "warn",
      { "extensions": [".js", ".jsx", ".tsx"] }
    ]
  },
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "react"]
}
